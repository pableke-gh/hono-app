{
	"source": {
		"include": ["./src"],
		"includePattern": "\\.(jsx|js|ts|tsx)$",
		"excludePattern": "(node_modules/|docs)"
	},
	"plugins": [
		"plugins/markdown",
		"node_modules/better-docs/category"
	],
	"opts": {
		"encoding": "utf8",
		"readme": "README.md",
		"destination": "./docs",
		"recurse": true,
		"verbose": true,
		"tutorials": "./docs/tutorials",
		"template": "node_modules/better-docs"
	}
}
